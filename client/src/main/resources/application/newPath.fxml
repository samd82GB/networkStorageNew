<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="application.NewPath"
            prefHeight="200.0" prefWidth="200.0">
    <children>

        <TextArea layoutX="10.0" layoutY="15.0" maxHeight="15.0" maxWidth="150.0" fx:id="textArea"/>
        <Button layoutX="10.0" layoutY="50.0" mnemonicParsing="false" text="OK" onAction="#enterNewPathDir"/>
        <Button layoutX="50.0" layoutY="50.0" mnemonicParsing="false" text="Отмена" cancelButton="true" onAction="#closeStage"/>


    </children>
</AnchorPane>
